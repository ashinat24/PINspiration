
<div class="row p-5">
  
    <div class="col-12 col-md-5 mt-2 text-res">
      <img src="/img/contact.jpeg" class="img-fluid relative-top img-res" style="top: 20px;" alt="contact-image" loading="lazy">
    </div>

    <div class="col-12 col-md-7">
          <div class="col-12">
            
            <div class="d-flex align-items-center h-custom-2 ms-xl-4 mt-xl-n5 pt-3 text-res" >
            
              <form  action="https://formspree.io/f/xvonjpno" method="POST">
                <h3 class="fw-normal mb-3 pb-3" style="letter-spacing: 1px;">Contact Us at PINspiration</h3>
                <div class="form-outline mb-4">
                    <label class="form-label"  for="form2Example18">Your name</label>
                  <input type="text" id="form2Example18" name="name" class="form-control form-control-lg" required/>
                </div>
                <div class="form-outline mb-4">
                    <label class="form-label"   for="form2Example18">Your Email address</label>
                  <input type="email" id="form2Example18" name="email" class="form-control form-control-lg"required/>
                </div>
                <div class="form-outline mb-4">
                    <label class="form-label"   for="form2Example18">Subject</label>
                  <input type="text" id="form2Example18" name="subject" class="form-control form-control-lg"required/>
                </div>
                <div class="form-outline mb-4">
                    <label for="description" class="form-label">Feedback</label>
                    <textarea name="message" id="description" class="form-control" cols="30" rows="4" minlength="10"></textarea>
                </div>
                <div class="pt-1 mb-4">
                  <button class="btn btn-info btn-lg btn-block bg-black text-white" type="submit">Send</button>
                </div>
              </form>
            </div>
          </div>
    </div>
  </div>
